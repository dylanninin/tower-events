<%= simple_form_for(@access) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :team %>
    <%= f.input :role %>
    <%= f.association :accessable %>
    <%= f.association :creator %>
    <%= f.input :deleted_at %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
