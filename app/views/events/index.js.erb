// Append new data
$("<%=j (render partial: 'events', object: @event_groups) %>")
  .appendTo($(".event-groups"));

// Update pagination link
<% if @events.last_page? %>
  $('.pagination').html("That's all, folks!");
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@events, 'Next Page', :remote => true) %>");
<% end %>
