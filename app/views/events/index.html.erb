<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Published</th>
      <th>Actor</th>
      <th>Verb</th>
      <th>Title</th>
      <th>Content</th>
      <th>Object</th>
      <th>Target</th>
      <th>Provider</th>
      <th>Generator</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.published %></td>
        <td><%= event.actor %></td>
        <td><%= event.verb %></td>
        <td><%= event.title %></td>
        <td><%= event.content %></td>
        <td><%= event.object %></td>
        <td><%= event.target %></td>
        <td><%= event.provider %></td>
        <td><%= event.generator %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
